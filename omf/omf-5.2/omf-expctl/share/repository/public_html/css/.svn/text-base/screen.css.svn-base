}

#tabs a:link, 
#tabs a:visited {
/*
    text-decoration:none;
*/
}

ul#mainTabs a {
    text-decoration: none;
}

ul#mainTabs a:hover {
    text-decoration: underline;
}


#tabs a:link, 
#tabs a:visited {
/*
    border-color:#CBEAC7 rgb(203, 234, 199) rgb(67, 124, 86);
    border-style:solid;
    border-width:1px;
    color:#FFFFFF;
*/
    background-color: transparent;
    color: black;
}

ul#mainTabs a.current:link, 
ul#mainTabs a.current:visited {
    color:#000000;
}

ul#mainTabs a.current:link, 
ul#mainTabs a.current:visited {
/*
    background:#E5E5E5 url(/images/tab-bg.gif) repeat-x scroll center top;
*/
    background:#E5E5E5;
    border-bottom:1px solid #E5E5E5;
    color:#222222;
    font-weight:bold;
    margin:0pt;
    text-decoration:none;
		margin-right:2pt;		
}

.adminTab {
    float: right;
    background:#E2F9E3;
}

/*--- PAGE NAV -- */
/*******************/
div.page_nav {
    background:#EDF3FE;
    border-top:1px solid #E1E1E1;
    border-top:1px solid #CCDEFE;    
    font-size:70%;
    overflow: auto;    
}

div.page_nav ul {
    list-style-image:none;
    list-style-position:outside;
    list-style-type:none;
    width:100%;
    margin:0pt;
    padding:0pt;
}

div.page_nav ul li {
    display:inline;
    margin:0pt;
    padding:0pt;
}


div.page_nav ul li a,
div.page_nav ul li a:link,
div.page_nav ul li a:visited,
div.page_nav ul li a:hover {
    border:medium none;
    color:#333333;
    display:block;
    float:left;
    font-weight:bold;
    padding:8px 30px;
    padding:6px 20px;
    position:relative;
    text-shadow:#FFFFFF 2px 2px;
    text-decoration: none;
    background-color: transparent;
}

div.page_nav ul li img.arrow {
	bottom:-8px;
	left:50%;
	margin-left:-6px;
	position:absolute;
	z-index:0;
}



div.page_nav ul li.selected a,
div.page_nav ul li.selected a:link, 
div.page_nav ul li.selected a:visited,
div.page_nav ul li.selected a:hover {
	background: #CCDEFE url(/resource/image/nav-tab-arrow.gif) no-repeat scroll 50% 100%;
}

div.page_nav ul li a:hover {
    text-decoration: underline;
}

div.page_nav ul li.selected a:hover {
    text-decoration: none;
}


