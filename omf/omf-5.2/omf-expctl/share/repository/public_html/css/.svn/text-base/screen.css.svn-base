}

#header h3, 
#header h3 a:link, 
#header h3 a:visited {
    color:#5A9E6C;
}

#header h3 a:hover {
    background-color: #1E5A28;
    color: white;
}

/*--- TABS ----*/

#tabs {
    height:25px;
    margin:0pt 30px 0pt 30px;
    padding:0pt;
}

#tabs ul {
    margin:0pt;
    padding:0pt;
}

#tabs li {
/*
    border:medium none;
*/
