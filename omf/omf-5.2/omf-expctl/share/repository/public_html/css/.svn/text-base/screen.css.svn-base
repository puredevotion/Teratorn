/****** LINKS ******/

a {
    padding:1px;
    text-decoration:underline;
}


a:link, 
a:visited {
    color:#437C56;
}


a:visited {
    color:#0033CC;
}

a:hover {
    background-color:#3300CC;
    color:#FFFFFF;
    text-decoration:none;
}

.admin {
    font-family:verdana;
    font-size:11px;
    font-weight:normal;
}

a.admin:link, 
a.admin:visited {
    color:#FF0000;
}

a.admin:hover, 
a.adminfull:hover {
    background:#FF0000 none repeat scroll 0%;
    color:#FFFFFF;
}

a.image:link, 
a.image:visited, 
a.image:hover {
    background:transparent none repeat scroll 0%;
    padding:0pt;
}

/***** PAGE STRUCTURE *****/


/*** PAGE HEADER ----*/


#header {
/*
    background:#003366 none repeat scroll 0%;
*/
